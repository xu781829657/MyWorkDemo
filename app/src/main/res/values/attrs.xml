<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RoundProgressBar">
        <attr name="roundColor" format="color"/>
        <attr name="roundProgressColor" format="color"/>
        <attr name="roundWidth" format="dimension"></attr>
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="max" format="integer"></attr>
        <attr name="textIsDisplayable" format="boolean"></attr>
        <attr name="style">
            <enum name="STROKE" value="0"></enum>
            <enum name="FILL" value="1"></enum>
        </attr>
    </declare-styleable>

    <declare-styleable name="RangeSeekBar">
  
        <attr name="ruleShortHeight" format="dimension"></attr>
        <attr name="ruleLongHeight" format="dimension"></attr>
        <attr name="defaultTextColor" format="color" />
        <attr name="rangeTextColor" format="color" />
        <attr name="partItemNum" format="integer"/>
        <attr name="totalPartItemNum" format="integer"/>
        <attr name="partNum" format="integer"/>
        <attr name="maxValue" format="integer"/>
        <attr name="fold" format="integer"/>
       
    </declare-styleable>
</resources>
